DROP TABLE IF EXISTS `recurringExceptions`;

CREATE TABLE `recurringExceptions` (
  `id` char(11) CHARACTER SET latin1 NOT NULL,
  `churchId` char(11) CHARACTER SET latin1 DEFAULT NULL,
  `recurringEventId` char(11) CHARACTER SET latin1 DEFAULT NULL,
  `eventId` char(11) CHARACTER SET latin1 DEFAULT NULL,
  `originalStart` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

